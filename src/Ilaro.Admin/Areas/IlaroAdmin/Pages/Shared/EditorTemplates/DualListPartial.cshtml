@model PropertyValue

<div class="col-md-6">
    @Html.DropDownList(Model.Property.Name, Model.GetPossibleValues(false), Model.Property, new { @class = "form-control dual-list", multiple = "true" })
    <span class="help-block" asp-if="@Model.Property.Description.HasValue()">@Model.Property.Description</span>
</div>
@Html.ValidationMessage(Model.Property.Name)
